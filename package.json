{
    "name": "react-webpack5-boilerplate",
    "version": "1.0.0",
    "description": "react-webpack5-boilerplate",
    "main": "index.js",
    "author": "rohit",
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
