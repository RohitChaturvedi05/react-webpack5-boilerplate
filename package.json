        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/preset-env": "^7.12.1",
        "@babel/preset-react": "^7.14.5",
        "@types/jest": "^27.0.1",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^27.2.0",
        "babel-loader": "^8.1.0",
        "babel-preset-es2016": "^6.24.1",
        "clean-webpack-plugin": "^3.0.0",
